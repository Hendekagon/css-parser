	     		  transition-property-value s0
			  transition-duration-value s0
			  (transition-timing-function-value s0)?
			  (transition-delay-value s0)?
			  <','>)*
			  ( s0
	     		  transition-property-value s0
			  transition-duration-value s0
			  (transition-timing-function-value s0)?
			  (transition-delay-value s0)?
			  )

vk = ('-o-' | '-webkit-' | '-moz-')?

