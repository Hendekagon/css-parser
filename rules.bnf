flex-flow = 'flex-flow' s0 <':'> s0 ((flex-direction-value | flex-wrap-value) s0)* (flex-direction-value | flex-wrap-value)

transition-property = vk 'transition-property' s0 <':'> s0 (transition-property-value | <','> | s0)+
transition-property-value = ('all' | 'none' | identifier | 'inherit')

