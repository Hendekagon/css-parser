(* transform = 'none' | 'translateZ(<length>)' | 'translateY(<translation-value>)' | 'translateX(<translation-value>)' | 'translate3d(<translation-value>' *)
(* text-emphasis = '[text-emphasis-color]' *)
(* icon = 'auto' | '[ One or more comma separated <url> values ]' *)

rule = selector <(s0|'\n'|'\t')*>
       		<'{'>
		<(s0|'\n'|'\t')*>

		( token <';'> <(s0|'\n'|'\t')*> )*

		token <';'?>

		<(s0|'\n'|'\t')*>
		<'}'>

<sep> = <s0 ':' s0>

selector = ((primitive-selector | relationship-selector) s0 <','> s0)* (primitive-selector | relationship-selector)
relationship-selector = nested | descendent | child | sibling
nested = (primitive-selector)+
descendent = primitive-selector s+ primitive-selector
child = primitive-selector s0* <'>'> s0* primitive-selector
sibling = primitive-selector s0* <'+'> s0* primitive-selector
primitive-selector = ((<'#'> id) | (<'.'> class) | html-element | any) (<':'> pseudoclass)?

any = '*'

html-element = ('html' | 'head' | 'title' | 'base' | 'isindex' | 'link' | 'meta' | 'style' | 'script' | 'noscript' | 'a' | 'abbr' | 'acronym' | 'address' | 'applet' | 'area' | 'article' | 'aside' | 'audio' | 'b' | 'base' | 'basefont' | 'bdi' | 'bdo' | 'bgsound' | 'big' | 'blink' | 'blockquote' | 'body' | 'br' | 'button' | 'canvas' | 'caption' | 'center' | 'cite' | 'code' | 'col' | 'colgroup' | 'command' | 'content' | 'data' | 'datalist' | 'dd' | 'del' | 'details' | 'dfn' | 'dialog' | 'dir' | 'div' | 'dl' | 'dt' | 'element' | 'em' | 'embed' | 'fieldset' | 'figcaption' | 'figure' | 'font' | 'footer' | 'form' | 'frame' | 'frameset' | 'head' | 'header' | 'hgroup' | 'hr' | 'html' | 'i' | 'iframe' | 'image' | 'img' | 'input' | 'ins' | 'isindex' | 'kbd' | 'keygen' | 'label' | 'legend' | 'li' | 'link' | 'listing' | 'main' | 'map' | 'mark' | 'marquee' | 'menu' | 'menuitem' | 'meta' | 'meter' | 'multicol' | 'nav' | 'nobr' | 'noembed' | 'noframes' | 'noscript' | 'object' | 'ol' | 'optgroup' | 'option' | 'output' | 'p' | 'param' | 'picture' | 'plaintext' | 'pre' | 'progress' | 'q' | 'rp' | 'rt' | 'rtc' | 'ruby' | 's' | 'samp' | 'script' | 'section' | 'select' | 'shadow' | 'small' | 'source' | 'spacer' | 'span' | 'strike' | 'strong' | 'style' | 'sub' | 'summary' | 'sup' | 'table' | 'tbody' | 'td' | 'template' | 'textarea' | 'tfoot' | 'th' | 'thead' | 'time' | 'title' | 'tr' | 'track' | 'tt' | 'u' | 'ul' | 'var' | 'video' | 'wbr' | 'xmp' | 'h1' | 'h2' | 'h3' | 'h4' | 'h5' | 'h6' ) / #'[a-zA-Z0-9]+'

pseudoclass = 'link' | 'visited' | 'active' | 'hover' | 'focus' | 'first-child' | 'last-child' | 'nth-child' | 'nth-last-child' | 'nth-of-type' | 'first-of-type' | 'last-of-type' | 'empty' | 'target' | 'checked' | 'enabled' | 'disabled'

class = #'[a-zA-Z\-\_]+'
id = #'[a-zA-Z\-\_]+'

token = (float | border-image-repeat | min-width | list-style-position | text-align-last | overflow-wrap | column-fill | border-top-color | line-height | text-orientation | text-emphasis-style | width | background-position | right | border-width | padding-bottom | max-width | background-attachment | top | transition-duration | border-color | counter-increment | background-repeat | column-width | justify-content | border-left-style | column-gap | height | page-break-after | outline-color | overflow-y | outline-width | text-emphasis-color | line-break | transform-origin | backface-visibility | margin-right | text-transform | vertical-align | align-items | word-break | break-inside | font-variant-caps | animation-play-state | writing-mode | flex-shrink | outline-offset | border-collapse | flex-direction | font-kerning | text-align | text-indent | white-space | text-decoration-color | background-origin | border-top-width | column-rule-width | border-right-color | text-justify | word-spacing | quotes | column-count | border-style | visibility | border-right-width | border-bottom-right-radius | align-self | page-break-before | position | column-span | animation-iteration-count | animation-direction | margin-top | border-bottom-left-radius | text-combine-horizontal | border-bottom-color | hanging-punctuation | text-decoration-skip | object-fit | list-style-type | z-index | flex-wrap | direction | text-overflow | border-top-right-radius | min-height | flex-grow | unicode-bidi | word-wrap | align-content | border-bottom-style | margin-bottom | border-left-color | font-weight | image-resolution | break-before | background-clip | border-right-style | list-style-image | background-size | object-position | page-break-inside | border-bottom-width | outline-style | order | font-size-adjust | animation-fill-mode | box-shadow | overflow | overflow-x | margin-left | font-size | empty-cells | text-decoration-line | background-color | clear-after | padding-right | letter-spacing | box-sizing | widows | image-orientation | perspective | border-spacing | display | break-after | font-style | bottom | all | text-decoration-style | perspective-origin | resize | clear | font-synthesis | pointer-events | box-decoration-break | text-underline-position | animation-delay | font-variant | tab-size | orphans | text-shadow | max-height | border-top-left-radius | color | border-left-width | padding-left | padding-top | opacity | text-emphasis-position | animation-duration | table-layout | left | caption-side | hyphens | transform-style | font-stretch | border-top-style | flex-basis | column-rule-style | border-bottom | border-top | border-left | border-right | border | flex-flow | border-image-slice | border-image-source| border-image-outset | border-image-repeat | border-image-width | border-image | transition-property | transition-timing-function | transition-duration | transition-delay | transition | font-family | columns | margin | clip-path | counter-reset | column-rule | column-rule-color | padding | outline | cursor | animation | animation-name | animation-play-state | content | font | text-decoration |  border-radius | background-image | flex | list-style | background | transform)

(* shorthand *)

transform = vk 'transform' sep ('none' | (transform-function | s0 )+)

transform-function = matrix | matrix3d | perspective-transform | rotate | rotate3d | rotateX | rotateY | rotateZ | scale | scale3d | scaleX | scaleY | scaleZ | skew | skewX | skewY | translate | translate3d | translateX | translateY | translateZ

matrix = 'matrix(' s0 number s0 ',' s0 number s0 ',' s0 number s0 ',' s0 number s0 ',' s0 number s0 ',' s0 number s0')'
matrix3d = 'matrix3d(' s0 number s0 ',' s0 number s0 ',' s0 number s0 ',' s0 number s0 ',' s0 number s0 ',' s0 number s0 ',' s0 number s0 ',' s0 number s0 ',' s0 number s0 ',' s0 number s0 ',' s0 number s0 ',' s0 number s0 ',' s0 number s0 ',' s0 number s0 ',' s0 number s0 ',' s0 number s0 ')'

perspective-transform = 'perspective(' s0 length s0 ')'
rotate = 'rotate(' s0 angle s0')'
rotate3d = 'rotate3d(' s0 number s0 ',' s0 number s0 ',' s0 number s0 ',' s0 angle s0')'
rotateX = 'rotateX(' s0 angle s0')'
rotateY = 'rotateY(' s0 angle s0')'
rotateZ = 'rotateZ(' s0 angle s0')'
scale = 'scale(' s0 number (s0 ',' s0 number)? s0 ')'
scale3d = 'scale3d(' s0 number s0 ',' s0 number s0 ',' s0 number s0 ')'
scaleX = 'scaleX(' s0 number s0 ')'
scaleY = 'scaleY(' s0 number s0 ')'
scaleZ = 'scaleZ(' s0 number s0 ')'
skew = 'skew(' s0 angle (s0 ',' s0 angle)? s0 ')'
skewX = 'skewX(' s0 angle s0 ')' 
skewY = 'skewY(' s0 angle s0 ')'
translate = 'translate(' s0 ( length | percentage ) (s0 ',' s0 ( length | percentage ))? s0 ')'
translate3d = 'translate3d(' s0 ( length | percentage ) s0 ',' s0 ( length | percentage ) s0 ',' s0 ( length | percentage ) s0 ')'
translateX = 'translateX(' s0 ( length | percentage ) s0 ')'
translateY = 'translateY(' s0 ( length | percentage ) s0 ')'
translateZ = 'translateZ(' s0 ( length | percentage ) s0 ')'

background = 'background' sep (
	     		  background-image-value | background-position-value (s0 '/' s0 background-size-value)? | background-repeat-value | background-attachment-value | background-clip-value | background-color-value | s0)+ 

list-style = 'list-style' sep (list-style-type-value | list-style-image-value | list-style-position-value | s0 )+

flex = vk 'flex' sep (flex-grow-value | flex-basis-value |
       	      	   flex-grow-value s0 flex-basis-value |
		   flex-grow-value s0 flex-shrink-value |
		   flex-grow-value s0 flex-shrink-value s0 flex-basis-value |
		   'none')

background-image = 'background-image' sep background-image-value
background-image-value = ('none' | 'inherit' | image)

border-radius = vk 'border-radius' sep ((
	      	all-radius |
		top-left-bottom-right-radius s0 top-right-bottom-left-radius |
		top-left-radius s0 top-right-bottom-left-radius s0 bottom-right-radius |
		top-left-radius s0 top-right-radius s0 bottom-right-radius s0 bottom-left-radius )
		(s0 '/' s0 (
	      	all-radius |
		top-left-bottom-right-radius s0 top-right-bottom-left-radius |
		top-left-radius s0 top-right-bottom-left-radius s0 bottom-right-radius |
		top-left-radius s0 top-right-radius s0 bottom-right-radius s0 bottom-left-radius ))? | 'inherit')

all-radius = length | percentage
top-left-bottom-right-radius = length | percentage
top-right-bottom-left-radius = length | percentage
top-left-radius = length | percentage
bottom-right-radius = length | percentage
top-right-radius = length | percentage
bottom-left-radius = length | percentage

text-decoration = 'text-decoration' sep (
		  ( 'inherit' | 'initial' | 'unset' | 'none') /
		  (text-decoration-color-value | text-decoration-style-value | text-decoration-line-value | s0)+ )

font = 'font' sep (
       ('caption' | 'icon' | 'menu' | 'message-box' | 'small-caption' | 'status-bar')* /
       ((font-style-value | font-variant-value | font-weight-value | (font-size-value (s0 <'/'> line-height-value)?) | generic-font-family | s0) / family-font-name)+  )

content = 'content' sep
	  ( string | url | count | 'attr(' s0 identifier s0 ')' | 'open-quote' | 'close-quote' | 'no-open-quote' | 'no-close-quote' | s0 )+ | 'normal' | 'none'

animation-name = vk 'animation-name' sep animation-name-value
animation-name-value = ('none' | identifier )

animation-play-state = vk 'animation-play-state' sep (animation-play-state-value | s0)+
animation-play-state-value = ('paused' | 'running')

animation = vk 'animation' sep (
	       (animation-name-value s0 animation-duration-value) |
	       (animation-name-value s0 animation-duration-value s0 animation-timing-function-value s0 animation-delay-value) |
	       (animation-name-value s0 animation-duration-value s0 animation-timing-function-value s0 animation-delay-value s0 animation-iteration-count-value s0 animation-direction-value s0 animation-fill-mode-value s0 animation-play-state-value) )

cursor = vk 'cursor' sep
       ( ( url s0 (integer s0 integer)? <','>? s0)* ( 'auto' | 'default' | 'none' | 'context-menu' | 'help' | 'pointer' | 'progress' | 'wait' | 'cell' | 'crosshair' | 'text' | 'vertical-text' | 'alias' | 'copy' | 'move' | 'no-drop' | 'not-allowed' | 'e-resize' | 'n-resize' | 'ne-resize' | 'nw-resize' | 's-resize' | 'se-resize' | 'sw-resize' | 'w-resize' | 'ew-resize' | 'ns-resize' | 'nesw-resize' | 'nwse-resize' | 'col-resize' | 'row-resize' | 'all-scroll' | 'zoom-in' | 'zoom-out' | 'grab' | 'grabbing' ) )

outline = 'outline' sep
	(outline-style-value | outline-width-value | outline-color-value | s0 )+

padding = 'padding' sep
	  (padding-value |
	  padding-horizontal s0 padding-vertical |
	  padding-top-value s0 padding-horizontal s0 padding-bottom-value |
	  padding-top-value s0 padding-right-value s0 padding-left-value s0 padding-bottom-value
	  ) | 'inherit'

padding-value = '0' | percentage | length
padding-vertical = padding-value
padding-horizontal = padding-value
column-rule = 'column-rule' sep (column-rule-width-value | column-rule-style-value | column-rule-color-value | s0)+

counter-reset = 'counter-reset' sep (identifier s0 integer s0?)+ | 'none' | 'inherit'

clip-path = 'clip-path' sep (url | 'none' | 'inherit')

margin = 'margin' sep (
       margin-all |
       margin-top-value s0 margin-hor-value |
       margin-top-value s0 margin-hor-value s0 margin-bottom-value |
       margin-top-value s0 margin-right-value s0 margin-bottom-value s0 margin-left-value ) | 'inherit'

margin-all = '0' | 'auto' | percentage | length
margin-hor-value = margin-all

columns = 'columns' sep (column-width-value | column-count-value | s0)+

font-family = 'font-family' sep ((generic-font-family / family-font-name) s0 <','> s0)* (generic-font-family / family-font-name)
family-font-name = ((custom-ident s)+ custom-ident) / custom-ident / string
generic-font-family = 'serif' | 'sans-serif' | 'monospace' | 'cursive' | 'fantasy'

border-bottom = 'border-bottom' s0 <':'> s0 (border-bottom-color-value | border-bottom-style-value | border-bottom-width-value | s0)+ 

border-top = 'border-top' s0 <':'> s0 (border-top-color-value | border-top-style-value | border-top-width-value | s0)+ 

border-left = 'border-left' s0 <':'> s0 (border-left-color-value | border-left-style-value | border-left-width-value | s0)+

border-right = 'border-right' s0 <':'> s0 (border-right-color-value | border-right-style-value | border-right-width-value | s0)+ 

border = 'border' s0 <':'> s0 (border-color-value | border-style-value | border-width-value | s0)+

border-image-slice = 'border-image-slice' s0 <':'> s0 border-image-slice-value
<border-image-slice-value> = (number | percentage | 'fill' | 'inherit' | s0)+

border-image-source = 'border-image-source' s0 <':'> s0 border-image-source-value
<border-image-source-value> = ('none' | image | 'inherit')

border-image-outset = 'border-image-outset' s0 <':'> s0 border-image-outset-value
<border-image-outset-value> = ((length | number| s0)+ | 'inherit')

border-image-repeat = 'border-image-repeat' s0 <':'> s0 border-image-repeat-value
<border-image-repeat-value> = (('stretch' | 'repeat' | 'round' | 'space' | s0 )+ |  'inherit')

border-image-width = 'border-image-width' s0 <':'> s0 border-image-width-value
<border-image-width-value> = ((length | percentage | number | 'auto' | s0)+ | 'inherit')

border-image = vk 'border-image' s0 <':'> s0 (border-image-source-value | border-image-slice-value | border-image-width-value | border-image-width-value | border-image-outset-value | border-image-repeat-value)+
